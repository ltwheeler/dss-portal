<h1>Listing app_bookmarks</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Url</th>
    <th>Uid</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_bookmarks.each do |app_bookmark| %>
  <tr>
    <td><%= app_bookmark.name %></td>
    <td><%= app_bookmark.description %></td>
    <td><%= app_bookmark.url %></td>
    <td><%= app_bookmark.uid %></td>
    <td><%= link_to 'Show', app_bookmark %></td>
    <td><%= link_to 'Edit', edit_app_bookmark_path(app_bookmark) %></td>
    <td><%= link_to 'Destroy', app_bookmark, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App bookmark', new_app_bookmark_path %>
